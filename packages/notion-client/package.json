	},
	"dependencies": {
		"@types/node-fetch": "2.6.12",
		"node-fetch": "2.7.0"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "0.17.3",
		"@repo/eslint-config": "workspace:*",
		"@repo/tools": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"typescript": "5.5.4",
		"vitest": "2.1.8"
	},
	"packageManager": "pnpm@10.3.0"
