  },
  "dependencies": {
    "@notionhq/client": "file:../..",
    "dotenv": "17.2.1"
  },
  "devDependencies": {
    "@types/node": "22.13.14",
    "prettier": "3.5.3",
    "ts-node": "10.8.2",
    "typescript": "5.5.4"
  }
