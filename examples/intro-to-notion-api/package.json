    "basic:2": "node --loader ts-node/esm basic/2-add-linked-block.ts",
    "basic:3": "node --loader ts-node/esm basic/3-add-styled-block.ts",
    "intermediate:1": "node --loader ts-node/esm intermediate/1-create-a-database.ts",
    "intermediate:2": "node --loader ts-node/esm intermediate/2-add-page-to-database.ts",
    "intermediate:3": "node --loader ts-node/esm intermediate/3-query-database.ts",
    "intermediate:4": "node --loader ts-node/esm intermediate/4-sort-database.ts",
    "intermediate:5": "node --loader ts-node/esm intermediate/5-upload-file.ts"
  },
  "repository": {
    "type": "git",
